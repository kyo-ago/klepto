{
	"name": "Klepto proxy",
	"version": "1.5",
	"manifest_version": 2,
	"minimum_chrome_version": "25",
	"icons": {
		"16": "logo.png",
		"128": "logo.png"
	},
	"app": {
		"background": {
			"scripts": [
				"/src/background.js"
			]
		}
	},
	"sandbox" : {
		"pages" : ["/html/sandbox.html"]
	},
	"permissions": [
		"storage",
		"contextMenus",
		{"fileSystem": ["write"]},
		{"socket": ["tcp-listen", "tcp-connect"]}
	]
}